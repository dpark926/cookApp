
<%= form_for @customer do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br><br>

  <%= f.label :city %>
  <%= f.text_field :city %><br><br>

  <%= f.label :profile_picture %>
  <%= f.text_field :image_url %><br><br>

  <%= f.submit %>
<% end %>
