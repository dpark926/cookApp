<!DOCTYPE html>
<html>
  <head>
    <title>FoodApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>
    <div id = "navi">
      <img id="home" src="https://iconmonstr.com/wp-content/g/gd/makefg.php?i=../assets/preview/2012/png/iconmonstr-home-10.png&r=0&g=0&b=0" alt="" height="40">
      <span class = "navlink" id="link1">discover</span>
      <% if logged_in? %>
        <div class = "rightnav">
          <span id = "cartcount">6</span>
          <img src ="https://cdns.iconmonstr.com/wp-content/assets/preview/2013/240/iconmonstr-shopping-cart-1.png" height="35px" id="shoppingcart">
          <img src = "<%= profilepic %>" width="50px" height="50px" class = "profilepic">
          <div class ="rightnavlinks">
            <span id = "settings">settings</span>
            <span id = "logout"><%= link_to "logout", logout_path %></span>
          </div>
        </div>
      <% end %>
    </div>
    <hr>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
