<style media="screen">
  .loginform{
    margin-top:140px;
  }
</style>
<div class="loginform">
<%= form_tag '/sessions' do %>
  <%= label_tag :'email' %> <br>
  <%= text_field_tag :email %> <br>
  <%= label_tag :'password' %> <br>
  <%= password_field_tag :password %> <br>
  <%= submit_tag "Sign In", class:"btn" %>
<% end %>
</div>
